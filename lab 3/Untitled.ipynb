    "#алгоритм для небольших последовательностей,
    "import sys\n",
    "import time\n",
    "\n",
    "class CompressedDNA:\n",
    "    def __init__(self, dna_sequence):\n",
    "        # Удаляем пробелы и символы переноса строки, преобразуем в верхний регистр\n",
    "        self.dna_sequence = dna_sequence.replace(' ', '').replace('\\n', '').upper()\n",
    "        # Проверяем допустимость символов\n",
    "        if not set(self.dna_sequence).issubset({'A', 'C', 'G', 'T'}):\n",
    "            raise ValueError(\"Последовательность содержит недопустимые символы!\")\n",
    "        self.size = len(self.dna_sequence)  # Сохраняем длину последовательности\n",
    "        self.__compress()\n",
    "    \n",
    "    def __compress(self):\n",
    "        start_time = time.time()  # Засекаем время начала компрессии\n",
    "        nucleotide_map = {'A': 0b00, 'C': 0b01, 'G': 0b10, 'T': 0b11}\n",
    "\n",
    "        self.bits = 0  # Начинаем с 0\n",
    "        for i, nucleotide in enumerate(self.dna_sequence):\n",
    "            self.bits <<= 2  # Сдвигаем биты влево на 2 позиции\n",
    "            self.bits |= nucleotide_map[nucleotide]\n",
    "\n",
    "        end_time = time.time()  # Засекаем время окончания компрессии\n",
    "        self.compression_time = end_time - start_time  # Вычисляем время компрессии\n",
    "    \n",
    "    def decompress(self):\n",
    "        nucleotide_map = {0b00: 'A', 0b01: 'C', 0b10: 'G', 0b11: 'T'}\n",
    "        sequence = []\n",
    "\n",
    "        temp = self.bits\n",
    "        for _ in range(self.size):\n",
    "            nucleotid_bits = temp & 0b11  # Получаем последние 2 бита\n",
    "            sequence.append(nucleotide_map[nucleotid_bits])\n",
    "            temp >>= 2  # Сдвигаем биты вправо на 2 позиции\n",
    "\n",
    "        return ''.join(sequence[::-1])  # Обращаем строку и соединяем в строку\n",
    "\n",
    "    def __str__(self):\n",
    "        return self.decompress()\n",
    "\n",
    "# Чтение последовательности из файла\n",
    "with open('dna_sequence.txt', 'r') as file:\n",
    "    original_dna_str = file.read()\n",
    "\n",
    "# Создаем экземпляр класса сжатой ДНК\n",
    "compressed_dna = CompressedDNA(original_dna_str)\n",
    "\n",
    "# Вывод размеров исходной и сжатой строки\n",
    "print(f'Исходная строка: {sys.getsizeof(original_dna_str)} байтов')\n",
    "print(f'Сжатая строка: {sys.getsizeof(compressed_dna.bits)} байтов')\n",
    "\n",
    "# Вывод времени компрессии\n",
    "print(f'Время компрессии: {compressed_dna.compression_time:.6f} секунд')\n",
    "\n",
    "decompressdna = compressed_dna.decompress()\n",
    "\n",
