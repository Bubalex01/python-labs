    "#алгоритм для небольших последовательностей,
    "import sys\n",
    "import time\n",
    "from concurrent.futures import ThreadPoolExecutor\n",
    "\n",
    "class CompressedDNA:\n",
    "    def __init__(self, dna_sequence):\n",
    "        self.dna_sequence = dna_sequence.replace(' ', '').replace('\\n', '').upper()\n",
    "        if not set(self.dna_sequence).issubset({'A', 'C', 'G', 'T'}):\n",
    "            raise ValueError(\"Последовательность содержит недопустимые символы!\")\n",
    "        \n",
    "        self.size = len(self.dna_sequence)\n",
    "\n",
    "        # Устанавливаем размер чанка на основе длины последовательности\n",
    "        self.chunk_size = self.calculate_chunk_size()\n",
    "\n",
    "        self.chunks = []\n",
    "        self.__compress()\n",
    "\n",
    "    def calculate_chunk_size(self):\n",
    "        # Определяем размер чанка пропорционально размеру последовательности\n",
    "        base_size = self.size // 500 #1000\n",
    "        return base_size\n",
    "\n",
    "    def __compress_chunk(self, chunk):\n",
    "        nucleotide_map = {'A': 0b00, 'C': 0b01, 'G': 0b10, 'T': 0b11}\n",
    "        bits = 0\n",
    "        \n",
    "        for nucleotide in chunk:\n",
    "            bits <<= 2\n",
    "            bits |= nucleotide_map[nucleotide]\n",
    "        \n",
    "        return bits\n",
    "    \n",
    "    def __compress(self):\n",
    "        start_time = time.time()\n",
    "        \n",
    "        # Разбиваем последовательность на чанки\n",
    "        with ThreadPoolExecutor() as executor:\n",
    "            chunked_sequences = [self.dna_sequence[i:i + self.chunk_size] for i in range(0, self.size, self.chunk_size)]\n",
    "            self.chunks = list(executor.map(self.__compress_chunk, chunked_sequences))\n",
    "        \n",
    "        end_time = time.time()\n",
    "        self.compression_time = end_time - start_time\n",
    "\n",
    "    def decompress(self):\n",
    "        nucleotide_map = {0b00: 'A', 0b01: 'C', 0b10: 'G', 0b11: 'T'}\n",
    "        sequence = []\n",
    "\n",
    "        for bit_chunk in self.chunks:\n",
    "            temp = bit_chunk\n",
    "            chunk_sequence = []\n",
    "            for _ in range(self.chunk_size):\n",
    "                nucleotid_bits = temp & 0b11\n",
    "                chunk_sequence.append(nucleotide_map[nucleotid_bits])\n",
    "                temp >>= 2\n",
    "            sequence.extend(chunk_sequence[::-1])\n",
    "\n",
    "        return ''.join(sequence)[:self.size]\n",
    "    \n",
    "    def __str__(self):\n",
    "        return self.decompress()\n",
    "\n",
    "# Чтение последовательности из файла\n",
    "with open('dna_sequence.txt', 'r') as file:\n",
    "    original_dna_str = file.read()\n",
    "\n",
    "# Создаем экземпляр класса сжатой ДНК\n",
    "compressed_dna = CompressedDNA(original_dna_str)\n",
    "\n",
    "# Вывод размеров исходной и сжатой строки\n",
    "print(f'Исходная строка: {sys.getsizeof(compressed_dna.dna_sequence)} байтов')\n",
    "print(f'Сжатая строка: {sys.getsizeof(compressed_dna.chunks)} байтов')\n",
    "\n",
    "# Вывод времени компрессии\n",
    "print(f'Время компрессии: {compressed_dna.compression_time:.6f} секунд')\n",
    "\n",
    "decompressdna = compressed_dna.decompress()\n",
    "\n",
    "# Проверка соответствия распакованной последовательности исходной\n",
    "if decompressdna == compressed_dna.dna_sequence:\n",
    "    print(\"Распакованная последовательность соответствует исходной.\")\n",
    "else:\n",
    "    print(\"Ошибка: последовательности не совпадают.\")"
